{
  "name": "Hello, World!",
  "description": "A intro to Chrome extensions.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": {
      "16": "/image/icon/helloWorld16.png",
      "48": "/image/icon/helloWorld48.png",
      "128": "/image/icon/helloWorld128.png"
    }
  }
}
